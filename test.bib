@article{campbell_fishing_nodate,
	title = {Fishing restrictions and remoteness deliver conservation outcomes for {Indonesia}'s coral reef fisheries},
	volume = {n/a},
	issn = {1755-263X},
	url = {https://conbio.onlinelibrary.wiley.com/doi/abs/10.1111/conl.12698},
	doi = {10.1111/conl.12698},
	abstract = {Coral reef fisheries depend on reef fish biomass to support ecosystem functioning and sustainable fisheries. Here, we evaluated coral reefs across 4,000 km of the Indonesian archipelago to reveal a large gradient of biomass, from {\textless}100 kg/ha to {\textgreater}17,000 kg/ha. Trophic pyramids characterized by planktivore dominance emerged at high biomass, suggesting the importance of pelagic pathways for reef productivity. Total biomass and the biomass of most trophic groups were higher within gear restricted and no-take management, but the greatest biomass was found on unmanaged remote reefs. Within marine protected areas (MPAs), 41.6\% and 43.6\% of gear restricted and no-take zones, respectively, met a global biomass target of 500 kg/ha, compared with 71.8\% of remote sites. To improve conservation outcomes for Indonesia's biodiverse and economically important coral reef fisheries, our results suggest to: (1) strengthen management within Indonesia's existing MPAs and (2) precautionarily manage remote reefs with high biomass.},
	language = {en},
	number = {n/a},
	urldate = {2020-04-11},
	journal = {Conservation Letters},
	author = {Campbell, Stuart J. and Darling, Emily S. and Pardede, Shinta and Ahmadia, Gabby and Mangubhai, Sangeeta and Amkieltiela and Estradivari and Maire, Eva},
	note = {\_eprint: https://conbio.onlinelibrary.wiley.com/doi/pdf/10.1111/conl.12698},
	keywords = {data-poor fisheries, food webs, gear restrictions, marine protected areas, small-scale fisheries, South East Asia},
	pages = {e12698},
	file = {Snapshot:C\:\\Users\\rian\\Zotero\\storage\\MLJUGUBX\\conl.html:text/html;Full Text PDF:C\:\\Users\\rian\\Zotero\\storage\\I846Y657\\Campbell et al. - Fishing restrictions and remoteness deliver conser.pdf:application/pdf}
