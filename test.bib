	year = {2019},
	pages = {eaay9969},
	file = {Tittensor et al. - 2019 - Integrating climate adaptation and biodiversity co.pdf:C\:\\Users\\rian\\Zotero\\storage\\ZTLXFIKE\\Tittensor et al. - 2019 - Integrating climate adaptation and biodiversity co.pdf:application/pdf}
